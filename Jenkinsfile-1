pipeline {
  agent {
    docker { image 'docker:latest' }
  }
  stages {
    stage('Test') {
      steps {
        sh 'docker --version'
      }
    }
    stage('Docker build grafana image') {
  steps {
    sh 'docker build -t grafana grafana/'
      }
    }
  }
}

